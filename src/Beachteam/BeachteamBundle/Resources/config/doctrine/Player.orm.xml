<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Beachteam\BeachteamBundle\Entity\Player" table="player">
    <indexes>
      <index name="fk_players_media1_idx" columns="media_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="facebookUrl" type="string" column="facebook_url" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="birthplace" type="string" column="birthplace" length="45" nullable="false"/>
    <field name="birthdate" type="datetime" column="birthdate" nullable="false"/>
    <field name="nationality" type="string" column="nationality" length="45" nullable="false"/>
    <field name="length" type="float" column="length" precision="10" scale="0" nullable="true"/>
    <field name="weight" type="float" column="weight" precision="10" scale="0" nullable="true"/>
    <field name="club" type="string" column="club" length="255" nullable="false"/>
    <field name="posiition" type="string" column="posiition" nullable="false"/>
    <many-to-one field="media" target-entity="Media">
      <join-columns>
        <join-column name="media_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
